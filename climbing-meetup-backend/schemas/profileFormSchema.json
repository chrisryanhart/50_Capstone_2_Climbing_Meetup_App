{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "minLength": 5,
        "maxLength": 30
      },
      "password": {
        "type": "string",
        "minLength": 4,
        "maxLength": 20
      },
      "name": {
        "type": "string",
        "maxLength": 20
      },
      "profile_image": {
        "type": "string",
        "maxLength": 20
      },
      "user_age": {
        "type": "integer",
        "minimum": 1
      },
      "user_gender": {
        "type": "string",
        "maxLength": 10
      },
      "is_parent": {
        "type": "boolean"
      },
      "has_dogs": {
        "type": "boolean"
      },
      "bio": {
        "type": "string"
      },
      "location_id": {
        "minimum": 1,
        "type": "integer"
        
      },
      "preferences": {
        "type": "object"
      }
    },
    "additionalProperties": false,
    "required": [
      "username",
      "password"
    ]
  }
